{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "dist"
	},
	"files": ["src/app.ts"],
	"exclude": ["node_modules", "dist"],
	"include": ["./src/**/*"]
}
